# Ban-Pick Client Environment Configuration Template
# Copy this file to .env and update values as needed

# Development server settings
PORT=3000
BROWSER=none

# Server API URL - Update this to match your server
REACT_APP_SERVER_URL=http://localhost:5000

# Build settings
GENERATE_SOURCEMAP=true

# Development flags
FAST_REFRESH=true

# Skip preflight check for Create React App
SKIP_PREFLIGHT_CHECK=true

# Production settings (uncomment for production builds)
# GENERATE_SOURCEMAP=false
# REACT_APP_SERVER_URL=https://your-production-server.com
